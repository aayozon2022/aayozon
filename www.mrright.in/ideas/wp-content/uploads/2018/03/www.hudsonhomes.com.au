/** responsive slider */

jQuery(window).bind("load", slick_carousel_resize);

jQuery(window).bind("resize", slick_carousel_resize);

jQuery(window).bind("orientationchange", slick_carousel_resize);



 function slick_carousel_resize () {

 	

    jQuery('.slide_three .slide_content').each(function () {

       

        var ratio = 0;  // Used for aspect ratio

        var width = jQuery(this).width();    // Current image width

        var height = jQuery(this).height();  // Current image height

		var windowWidth = $(window).width();



        var realwidth = jQuery('#banner').length > 0 ? jQuery('#banner').width() : window.innerWidth;



        if( jQuery('.slide_content').length > 0 ){

        	// for single listings

        	realwidth = 740;

        }

       		



        	ratio = realwidth / width;   // get ratio for scaling image

            

            height 	= height * ratio;    // Reset height to match scaled image

            width 	= width * ratio;    // Reset width to match scaled image

			

			if ($(window).width() > 1000) {

			    jQuery(this).css("width", "1000px"); // Set new width

				jQuery(this).css("height", "515px");  // Scale height based on ratio

				console.log(realwidth+" : "+width + ' | '+ height);

			}

			else {

			    jQuery(this).css("width", windowWidth); // Set new width

				jQuery(this).css("height", "515px");  // Scale height based on ratio

				console.log(realwidth+" : "+width + ' | '+ height);

			}

			

            

    });



	 

	 

}





